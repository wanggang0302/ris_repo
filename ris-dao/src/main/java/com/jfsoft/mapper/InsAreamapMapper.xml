	</resultMap>

	<insert id="insert" parameterType="com.jfsoft.model.InsAreamap" >
		insert into INS_AREAMAP (ID, CHECKNO, PUSHCODE, 
			AREACODE)
		values (#{id,jdbcType=OTHER}, #{checkno,jdbcType=DECIMAL}, #{pushcode,jdbcType=DECIMAL}, 
			#{areacode,jdbcType=OTHER})
	</insert>

	<insert id="insertSelective" parameterType="com.jfsoft.model.InsAreamap" >
		insert into INS_AREAMAP
		<trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="id != null" >
				ID,
			</if>
			<if test="checkno != null" >
				CHECKNO,
			</if>
			<if test="pushcode != null" >
				PUSHCODE,
			</if>
			<if test="areacode != null" >
				AREACODE,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides="," >
			<if test="id != null" >
				#{id,jdbcType=OTHER},
			</if>
			<if test="checkno != null" >
				#{checkno,jdbcType=DECIMAL},
			</if>
			<if test="pushcode != null" >
				#{pushcode,jdbcType=DECIMAL},
			</if>
			<if test="areacode != null" >
				#{areacode,jdbcType=OTHER},
			</if>
		</trim>
	</insert>

</mapper>