<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jfsoft.mapper.TPermissionMapper" >
	<resultMap id="BaseResultMap" type="com.jfsoft.model.TPermission" >
		<result column="id" property="id" jdbcType="INTEGER" />
		<result column="permissionname" property="permissionname" jdbcType="VARCHAR" />
		<result column="role_id" property="roleId" jdbcType="INTEGER" />
	</resultMap>

	<sql id="Base_Column_List" >
		id, permissionname, role_id
	</sql>

	<insert id="insert" parameterType="com.jfsoft.model.TPermission" >
		insert into t_permission (id, permissionname, role_id
			)
		values (#{id,jdbcType=INTEGER}, #{permissionname,jdbcType=VARCHAR}, #{roleId,jdbcType=INTEGER}
			)
	</insert>

	<insert id="insertSelective" parameterType="com.jfsoft.model.TPermission" >
		insert into t_permission
		<trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="id != null" >
				id,
			</if>
			<if test="permissionname != null" >
				permissionname,
			</if>
			<if test="roleId != null" >
				role_id,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides="," >
			<if test="id != null" >
				#{id,jdbcType=INTEGER},
			</if>
			<if test="permissionname != null" >
				#{permissionname,jdbcType=VARCHAR},
			</if>
			<if test="roleId != null" >
				#{roleId,jdbcType=INTEGER},
			</if>
		</trim>
	</insert>

</mapper>
